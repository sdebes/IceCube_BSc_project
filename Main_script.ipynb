   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "from tcn import TCN\n",
    "from tensorflow import keras\n",
    "import tensorflow as tf\n",
    "from sklearn import metrics\n",
    "from HomemadeFuncs import ROC_plot\n",
    "import sqlite3\n",
    "import time\n",
    "from Dataloader import *"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Parameters"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "N_muons = 80000\n",
    "N_epochs = 10              #Prøvede 100 epochs men ændrede ikke noget\n",
    "N_filters = 64\n",
    "Kernel_size = 2\n",
    "lr = 0.001\n",
    "N_features = 5\n",
    "batch_size = 1000           #200+ giver OOM (Out of memory på min pc)\n",
    "input_shape = (batch_size, None, 5)\n",
    "output_dim = 2\n",
    "\n",
    "params  = {'optimizer': tf.keras.optimizers.Adam(learning_rate=lr),\n",
    "           'loss'     : tf.keras.losses.BinaryCrossentropy(),\n",
    "           'metrics'  : [tf.keras.metrics.AUC() , tf.keras.metrics.BinaryAccuracy()]}"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "db_file = './rasmus_classification_muon_3neutrino_3mio.db'\n",
    "all_events = pd.read_csv('mine_events.csv').values\n",
    "events = all_events[0:N_muons]\n",
    "events = np.reshape(events, (N_muons,))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "3315"
      ]
     },
     "execution_count": 5,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# Alle events skal paddes til samme længde, i.e. længden af det længste event\n",
    "max_length = 1\n",
    "for event in events:\n",
    "    features, _ = PullFromDB(event, db_file)\n",
    "#    print(features.shape[0])\n",
    "    if features.shape[0] > max_length:\n",
    "        max_length = features.shape[0]\n",
    "max_length"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Model Architecture"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "def tcn_model_26():\n",
    "    i = tf.keras.Input(batch_shape = (None, max_length,5))\n",
    "    o = TCN(\n",
    "        nb_filters = N_filters,\n",
    "        kernel_size = Kernel_size,\n",
    "        #nb_stacks = 1,\n",
    "        #dilations=[1, 2, 4, 8, 16, 32],\n",
    "        #padding=\"causal\",\n",
    "        #use_skip_connections = False,\n",
    "        dropout_rate = 0.2,
    "        #return_sequences = False,\n",
    "        activation = tf.keras.activations.tanh,#tf.keras.layers.LeakyReLU()#\n",
    "        #kernel_initializer=\"he_normal\",\n",
    "        #use_layer_norm=False,\n",
    "        #use_batch_norm = False,\n",
    "     )(i)\n",
    "    o = tf.keras.layers.Dense(N_filters, activation = \"relu\")(o),
    "    o = tf.keras.layers.Dropout(0.2)(o),
    "    o = tf.keras.layers.Dense(N_filters, activation = \"relu\")(o),
    "    \n",
    "    o = tf.keras.layers.Dense(output_dim, activation = 'sigmoid')(o)\n",
    "    model = tf.keras.models.Model(inputs=[i], outputs=[o])\n",
    "    return model"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "model = tcn_model_26()\n",
    "model.compile(optimizer=params['optimizer'], loss=params['loss'], metrics= params['metrics'])"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Training"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "def Train():\n",
    "    with tf.device('/GPU:0'):     #GPU aktivering\n",
    "        a,b = 0, batch_size\n",
    "        \n",
    "        for i in range(int(N_muons/batch_size)):\n",
    "            input_feature_list = []\n",
    "            input_targets_list = []\n",
    "            \n",
    "        #Trækker en batch af events ud  \n",
    "            for event in events[a:b]:\n",
    "                features, truth = PullFromDB(event, db_file, E_threshold=0)\n",
    "                input_feats = TransformFeaturesToInput(features)\n",
    "                input_feature_list.append(input_feats)\n",
    "                input_targs = TransformTruthToInput(truth)\n",
    "                input_targets_list.append(input_targs)\n",
    "                \n",
    "        #Padder events, så hver batch har samme dimensioner\n",
    "            padded_input = []\n",
    "    \n",
    "            for feature in input_feature_list:\n",
    "                    temp = np.zeros([1, max_length, 5])\n",
    "                    temp[:, :feature.shape[1], :] = feature\n",
    "                    padded_input.append(temp)        \n",
    "                    padded_features = np.concatenate(padded_input)\n",
    "                    padded_target = np.concatenate(input_targets_list)\n",
    "    \n",
    "        #Træner på \"batch_size\" events af gangen\n",
    "            model.fit(padded_features, padded_target, epochs = N_epochs, verbose=0, batch_size = batch_size)\n",
    "            \n",
    "        # Print progress\n",
    "            if i % 10 ==0:\n",
    "                print((1+i)*batch_size,'of',N_muons,'||', np.round(100*(batch_size*(i+1)/(N_muons)),2), '%')\n",
    "\n",
    "            a += batch_size\n",
    "            b += batch_size\n",
    "    print('Done')"
   ]
  },
